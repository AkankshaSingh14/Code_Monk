#include<stdio.h>
int main()
{
	int t;
	scanf("%d",&t);
	if(t>=1 && t<=20)
	{
	while(t--)
	{
		int n,k,i,l,last;
		int ar[10000];
		scanf("%d %d",&n,&k);
		for(i=0;i<n;i++)
		{
			scanf("%d",&ar[i]);
		}
		for(i = 0; i < k; i++)
		{   
            last = ar[n-1];  
      		for(l = n-1; l > 0; l--)
				ar[l] = ar[l-1];  
        	ar[0] = last;  
    	}
		for(i=0;i<n;i++)
		{
			printf("%d ",ar[i]);
		}
	}
	}
}
